#!/bin/bash

# https://docs.docker.com/docker-cloud/builds/advanced/

function add_tag() {
  docker tag $IMAGE_NAME $DOCKER_REPO:$1
  docker push $DOCKER_REPO:$1
}

add_tag latest
add_tag swift-4
add_tag swift-4.1
add_tag swift-4.1.3
add_tag ${SOURCE_BRANCH}
add_tag ${SOURCE_BRANCH}_swift-4
add_tag ${SOURCE_BRANCH}_swift-4.1
add_tag ${SOURCE_BRANCH}_swift-4.1.3

exit $?
